    "@types/node-cron": "^3.0.11",
    "@types/node-windows": "^0.1.6",
    "@types/nodemailer": "^6.4.17",
    "@types/yargs": "^17.0.33",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "axios": "^0.29.0",
    "cheerio": "^1.0.0",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
